&lt;span class="cp"&gt;[&lt;/span&gt;&lt;span class="nb"&gt;s&lt;/span&gt;&lt;span class="cp"&gt;]&lt;/span&gt;   sel        &lt;span class="nt"&gt;&amp;lt;Selector&lt;/span&gt; &lt;span class="na"&gt;xpath=&lt;/span&gt;&lt;span class="s"&gt;None&lt;/span&gt; &lt;span class="na"&gt;data=&lt;/span&gt;&lt;span class="s"&gt;u&amp;#39;&amp;lt;html&lt;/span&gt;&lt;span class="nt"&gt;&amp;gt;&amp;lt;head&amp;gt;&amp;lt;script &lt;/span&gt;&lt;span class="na"&gt;type=&lt;/span&gt;&lt;span class="s"&gt;&amp;quot;text/javascrip&amp;#39;&lt;/span&gt;&lt;span class="nt"&gt;&amp;gt;&lt;/span&gt;
