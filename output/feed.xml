&lt;span class="cp"&gt;[&lt;/span&gt;&lt;span class="nb"&gt;s&lt;/span&gt;&lt;span class="cp"&gt;]&lt;/span&gt;   spider     &lt;span class="nt"&gt;&amp;lt;Spider&lt;/span&gt; &lt;span class="err"&gt;&amp;#39;&lt;/span&gt;&lt;span class="na"&gt;default&lt;/span&gt;&lt;span class="err"&gt;&amp;#39;&lt;/span&gt; &lt;span class="na"&gt;at&lt;/span&gt; &lt;span class="na"&gt;0x1bd5d10&lt;/span&gt;&lt;span class="nt"&gt;&amp;gt;&lt;/span&gt;
